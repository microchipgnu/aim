{
  "name": "@aim-sdk/core",
  "version": "0.0.33",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.0.6",
    "@ai-sdk/openai": "^1.0.11",
    "@markdoc/markdoc": "^0.4.0",
    "@openrouter/ai-sdk-provider": "0.2.1",
    "@sebastianwessel/quickjs": "^1.3.0",
    "ai": "^4.1.1",
    "browser-or-node": "^3.0.0",
    "chrome-ai": "^1.11.1",
    "js-yaml": "^4.1.0",
    "langchain": "^0.3.15",
    "nanoid": "^5.0.9",
    "ollama-ai-provider": "^1.0.0",
    "react": "^19.0.0",
    "rxjs": "^7.8.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@aim-sdk/tsconfig": "*",
    "@types/js-yaml": "^4.0.9",
    "@types/react": "^19.0.7",
    "tsup": "^8.4.0",
    "typescript": "^5.7.3",
    "@types/node": "^20.17.19",
    "@types/bun": "^1.2.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
