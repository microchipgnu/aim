{
	"name": "@aim-sdk/plugins-get-tools",
	"version": "0.0.0",
	"license": "Apache-2.0",
	"sideEffects": false,
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": ["dist/**/*", "CHANGELOG.md"],
	"scripts": {
		"build": "tsup",
		"clean": "rm -rf dist",
		"type-check": "tsc --noEmit",
		"lint": "biome lint",
		"format": "biome check --write",
		"test": "bun test",
		"dev": "tsup --watch"
	},
	"exports": {
		"./package.json": "./package.json",
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		}
	},
	"dependencies": {
		"@aim-sdk/core": "workspace:*"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@aim-sdk/tsconfig": "workspace:*",
		"tsup": "^8.4.0",
		"typescript": "^5.7.3",
		"@types/node": "^20.17.19"
	},
	"engines": {
		"node": ">=18"
	},
	"publishConfig": {
		"access": "public"
	}
}
