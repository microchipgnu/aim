    "@goat-sdk/wallet-evm": "^0.2.8",
    "@goat-sdk/wallet-viem": "^0.2.9",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@aim-sdk/tsconfig": "*",
    "tsup": "^8.4.0",
    "typescript": "^5.7.3",
    "@types/node": "^20.17.19"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  }
