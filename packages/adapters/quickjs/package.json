{
  "name": "@aim-sdk/adapters-quickjs",
  "version": "0.0.3",
  "private": true,
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@aim-sdk/core": "*",
    "@sebastianwessel/quickjs": "^1.3.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@aim-sdk/tsconfig": "*",
    "tsup": "^8.4.0",
    "typescript": "^5.7.3",
    "@types/node": "^20.17.19"
  }
}
