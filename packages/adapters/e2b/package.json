    "@biomejs/biome": "1.9.4",
    "@aim-sdk/tsconfig": "*",
    "tsup": "^8.4.0",
    "typescript": "^5.7.3",
    "@types/node": "^20.17.19"
  },
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
