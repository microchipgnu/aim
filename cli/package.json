{
  "name": "aimx",
  "description": "A terminal tool for AIM projects",
  "version": "0.1.17",
  "private": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "aimx": "dist/index.js"
  },
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "bun run typecheck && bun run copy-wasm && bun build ./index.ts --outdir ./dist --target node",
    "build-binary": "bun run typecheck && bun build ./index.ts --compile --outfile aimx",
    "typecheck": "tsc --noEmit",
    "version": "changeset version",
    "copy-wasm": "mkdir -p dist && cp node_modules/@wasmer/sdk/dist/*.wasm dist/",
    "release": "bun run cleanup && bun run build && changeset publish",
    "cleanup": "rm -rf dist"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^22.10.2",
    "@changesets/cli": "^2.27.11"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@aim-sdk/compiler": "0.1.8",
    "@aim-sdk/runtime": "0.1.8",
    "@aim-sdk/server": "0.1.7",
    "@wasmer/sdk": "^0.9.0",
    "commander": "^12.1.0",
    "dotenv": "^16.4.7",
    "inquirer": "^12.3.0"
  }
}
