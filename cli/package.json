{
  "name": "aimx",
  "description": "A terminal tool for AIM projects",
  "version": "0.1.32",
  "private": false,
  "type": "commonjs",
  "bin": {
    "aimx": "dist/index.cjs"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "dist/fonts"
  ],
  "scripts": {
    "build": "rm -rf dist && tsup --config tsup.config.ts",
    "build-binary": "bun run typecheck && bun build ./index.ts --compile --outfile aimx",
    "typecheck": "tsc --noEmit",
    "version": "changeset version",
    "copy-wasm": "mkdir -p dist && cp node_modules/@wasmer/sdk/dist/*.wasm dist/",
    "release": "bun run cleanup && bun run build && changeset publish",
    "cleanup": "rm -rf dist"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.11",
    "@types/bun": "latest",
    "@types/express": "^5.0.0",
    "@types/figlet": "^1.7.0",
    "@types/node": "^22.10.2",
    "tsup": "^8.3.5"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@aim-sdk/core": "0.0.6",
    "@markdoc/markdoc": "^0.4.0",
    "chalk": "^5.4.1",
    "commander": "^12.1.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "inquirer": "^12.3.0",
    "ora": "^8.1.1"
  }
}
