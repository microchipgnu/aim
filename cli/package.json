{
  "name": "aimx",
  "description": "A terminal tool for AIM projects",
  "version": "0.1.44",
  "private": false,
  "type": "commonjs",
  "bin": {
    "aimx": "dist/index.cjs"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "dist/ui"
  ],
  "scripts": {
    "build": "rm -rf dist && tsup --config tsup.config.ts",
    "dev": "bun run --watch ./index.ts serve -d ./test -p 3001",
    "dev:test": "concurrently \"bun run dev\" \"wait-on dist/index.mjs && bun --watch run ./dist/index.mjs start -d ../start-here/examples/ --ui\"",
    "build-binary": "bun run typecheck && bun build ./index.ts --compile --outfile aimx",
    "typecheck": "tsc --noEmit",
    "version": "changeset version",
    "copy-wasm": "mkdir -p dist && cp node_modules/@wasmer/sdk/dist/*.wasm dist/",
    "release": "bun run cleanup && bun run build && changeset publish",
    "cleanup": "rm -rf dist",
    "version:beta": "changeset pre enter beta && changeset version",
    "release:beta": "changeset pre exit && bun run build && changeset publish --tag beta",
    "version:stable": "changeset pre exit && changeset version"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.11",
    "@types/bun": "latest",
    "@types/express": "^5.0.0",
    "@types/figlet": "^1.7.0",
    "@types/node": "^22.10.2",
    "concurrently": "^9.1.2",
    "tsup": "^8.3.5"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@aim-sdk/core": "0.0.19",
    "@markdoc/markdoc": "^0.4.0",
    "@modelcontextprotocol/sdk": "^1.4.1",
    "@types/cors": "^2.8.17",
    "@types/js-yaml": "^4.0.9",
    "chalk": "^5.4.1",
    "commander": "^12.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "inquirer": "^12.3.0",
    "js-yaml": "^4.1.0",
    "nanoid": "^5.0.9",
    "openapi-mcp-server": "^1.2.0-beta04",
    "openapi-types": "^12.1.3",
    "ora": "^8.1.1",
    "vite": "^6.0.7",
    "wait-on": "^8.0.2"
  }
}