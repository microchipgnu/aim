{
  "name": "@aim-sdk/runtime",
  "version": "0.1.4",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.0.6",
    "@ai-sdk/openai": "^1.0.11",
    "@aim-sdk/compiler": "^0.1.3",
    "@openrouter/ai-sdk-provider": "^0.0.6",
    "@wasmer/sdk": "^0.9.0",
    "ai": "^4.0.22",
    "chrome-ai": "^1.11.1",
    "effector": "^23.2.3",
    "nanoid": "^5.0.9",
    "ollama-ai-provider": "^1.0.0",
    "replicate": "^1.0.1",
    "zod": "^3.24.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "rm -rf dist && tsc -p tsconfig.build.json && bun build ./index.ts --outdir ./dist --target node"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.js"
    }
  },
  "files": [
    "dist"
  ]
}